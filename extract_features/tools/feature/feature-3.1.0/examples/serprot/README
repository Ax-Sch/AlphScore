Evaluation
---
SeqFEATURE: xportal1:/Users/mliang/Projects/Research/SerineProteasePaper
Pfam: xportal1:/Users/mliang/Projects/Research/SerineProteasePaper/hmmer

# Converting from old .site file to .ptfs
site2tab trypsin_ser_og.site | awk '$5=="site"' > trypsin_ser_og.pos.ptf
site2tab trypsin_ser_og.site | awk '$5=="nonsite"' > trypsin_ser_og.neg.ptf

# Building the model
featurize -P trypsin_ser_og.pos.ptf > trypsin_ser_og.pos.ff
featurize -P trypsin_ser_og.neg.ptf > trypsin_ser_og.neg.ff
buildmodel trypsin_ser_og.pos.ff trypsin_ser_og.neg.ff > trypsin_ser_og.model

# Scanning the ASTRAL 40 database
atomselector.py -r ser -a og -f astral40.ids > astral40.ptf
featurize -P astral40.ptf > astral40.ff
scoreit trypsin_ser_og.model astral40.ff > astral40.hits
sort -k2 -n astral40.hits > astral40.hits.sorted

# Scanning one structure
atomselector.py -r ser -a og 1bqy > 1bqy.ptf
featurize -P 1bqy.ptf > 1bqy.ff
scoreit -a trypsin_ser_og.model 1bqy.ff > 1bqy.hits
sort -k2 -n 1bqy.hits > 1bqy.hits.sorted
